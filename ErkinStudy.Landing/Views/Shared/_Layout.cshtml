<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"]</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" />
</head>
<body>
    <header>
        <nav class="navbar bg-white">
            <div class="container">
                <div class="row">
                    <div class="logo"></div>
                    <div class="logo-title">
                        <span class="logo-title-empty"></span>
                        <span>ErkinSTUDY</span>
                        <span>Bilimińmen Erkinsiń - Knowledge frees</span>
                    </div>
                </div>
                <div class="row justify-content-end navbar-buttons">
                    <a href="" class="btn">Erkin oqýshy</a>
                    <a href="" class="btn">UBT Hub</a>
                </div>
            </div>
        </nav>
    </header>
    <div class="main">
            @RenderBody()
    </div>

    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2020 - ErkinStudy.Landing - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
        </div>
    </footer>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @RenderSection("Scripts", required: false)
</body>
</html>
